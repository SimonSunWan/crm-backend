# Docker环境配置文件
# 在docker-compose.yml中使用此文件或将配置复制到.env文件

# 数据库配置
DATABASE_URL=postgresql://postgres:123456@postgres:5432/postgres

# Redis配置
REDIS_URL=redis://redis:6379/0
REDIS_PASSWORD=
REDIS_DB=0
REDIS_MAX_CONNECTIONS=20
REDIS_SOCKET_TIMEOUT=5
REDIS_SOCKET_CONNECT_TIMEOUT=5

# 安全配置
SECRET_KEY=your-super-secret-key-change-this-in-production-environment

# CORS配置
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8080","http://localhost:5173"]

# 应用配置
DEBUG=true
PROJECT_NAME=CRM后端系统
VERSION=1.0.0
DESCRIPTION=CRM后端系统API
API_PRE_STR=/api

# JWT配置
ACCESS_TOKEN_EXPIRE_MINUTES=720

# 缓存配置
CACHE_DEFAULT_TTL=3600
CACHE_USER_TTL=1800
CACHE_MENU_TTL=7200
CACHE_DICTIONARY_TTL=3600
